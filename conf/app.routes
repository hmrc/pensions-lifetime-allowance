# microservice specific routes

POST       /individuals/:nino/protections       @controllers.DefaultCreateProtectionsController.applyForProtection(nino: String)
GET        /individuals/:nino/protections/count @controllers.DefaultReadProtectionsController.readExistingProtectionsCount(nino: String)
GET        /individuals/:nino/protections       @controllers.DefaultReadProtectionsController.readExistingProtections(nino: String)
PUT        /individuals/:nino/protections/:id   @controllers.DefaultAmendProtectionsController.amendProtection(nino: String, id: String)

GET        /psalookup/:psaRef/:ltaRef           @controllers.DefaultLookupController.psaLookup(psaRef, ltaRef)
